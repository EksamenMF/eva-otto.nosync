<article>
  <div class="billedecontainer">
    <img src="../public/Eva.webp" alt="Eva" class="right" />
    <img src="../public/otto2.webp" alt="Otto" class="left" />
  </div>
</article>

<style>
  .billedecontainer {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
    position: relative;
    gap: 5px;
    margin-bottom: 5%;
  }

  .left,
  .right {
    width: 35%;
    height: auto;
    object-fit: cover;
    border-radius: 5px;
    position: relative;
  }

  .left {
    right: 5%;
    margin-top: 10%;
  }

  .right {
    z-index: 1;
  }

  @media (max-width: 768px) {
    .billedecontainer {
      flex-direction: column;
    }

    .left,
    .right {
      width: 80%;
      margin: 0;
    }
  }

  @media (max-width: 480px) {
    .left,
    .right {
      width: 65%;
      margin: 0;
    }
  }
</style>
